		<template>
			<think><set name="which"> <star/></set></think>
			<condition name="which" value="Vietnam War">19 years, 5 months, 4 weeks and 1 day</condition>
			<condition name="which" value="Cold War">The Cold War lasted for a total of 45 years</condition>
			<condition name="which" value="World War II">6 yeard and one day</condition>
			<condition name="which" value="World War I">4 years, 3 months and 2 weeks</condition>
